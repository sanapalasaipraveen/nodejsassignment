<!DOCTYPE html>
<html>
<style>
    th,
    td {
        border: 1px solid black;
        padding: 5px 25px;
    }
    th {
        color: #dc143c;
    }
    table {
        border: 1px solid black;
        border-radius: 5px;
        margin: 20px;
    }
    a {
        text-decoration: none;
        color: #4400ff;
    }
    a:hover {
        color: rgba(64, 119, 8, 0.877);
        cursor: pointer;
    }
    .content {
        background-color: rgb(82, 200, 236);
        display: flex;
        flex-direction: column;
        align-items: center;
        height: 97vh;
    }
    .main-con {
        border-radius: 5px;
        color: #f0f8ff;
    }
</style>
<body>
    <div class="content">
        <div class="main-con">
            <table style="width:100%">
                <tr>
                    <th>Name</th>
                    <th>Age</th>
                    <th>Email</th>
                    <th>Profession</th>
                    <th>City</th>
                </tr>
                <% users.forEach( user=> {%>
                    <tr>
                        <td>
                            <%=user.name%>
                        </td>
                        <td>
                            <%=user.age%>
                        </td>
                        <td>
                            <%=user.email%>
                        </td>
                        <td>
                            <%=user.profession%>
                        </td>
                        <td>
                            <%=user.city%>
                        </td>
                    </tr>
                    <%})%>
            </table>
        </div>
        <a href='/form'>Add user</a>
    </div>
</body>
</html>